{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--log-level=debug",
        "--port=8080",
        "--host=0.0.0.0"
      ],
      "env": {
        "PYTHONPATH": "/usr/src/app/app"
      },
      "subProcess": true
    }
  ]
}
